  SimulatorInterfacePackets_0.position_y,
  SimulatorInterfacePackets_0.position_z,
  SimulatorInterfacePackets_0.velocity_x,
  SimulatorInterfacePackets_0.velocity_y,
  SimulatorInterfacePackets_0.velocity_z,
  SimulatorInterfacePackets_0.rotation_pitch,
  SimulatorInterfacePackets_0.rotation_yaw,
  SimulatorInterfacePackets_0.rotation_roll,
  SimulatorInterfacePackets_0.relativeOrientationToNorth,
  SimulatorInterfacePackets_0.angularVelocity_x,
  SimulatorInterfacePackets_0.angularVelocity_y,
  SimulatorInterfacePackets_0.angularVelocity_z,
  SimulatorInterfacePackets_0.bodyHeight,
  SimulatorInterfacePackets_0.engineRPM,
