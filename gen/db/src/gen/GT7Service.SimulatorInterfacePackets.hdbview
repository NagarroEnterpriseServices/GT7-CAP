  SimulatorInterfacePackets_0.lastLapTime,
  SimulatorInterfacePackets_0.timeOfDayProgression,
  SimulatorInterfacePackets_0.preRaceStartPositionOrQualiPos,
  SimulatorInterfacePackets_0.numCarsAtPreRace,
  SimulatorInterfacePackets_0.minAlertRPM,
  SimulatorInterfacePackets_0.maxAlertRPM,
  SimulatorInterfacePackets_0.calculatedMaxSpeed,
  SimulatorInterfacePackets_0.flags,
  SimulatorInterfacePackets_0.currentGear,
  SimulatorInterfacePackets_0.suggestedGear,
  SimulatorInterfacePackets_0.throttle,
  SimulatorInterfacePackets_0.brake,
  SimulatorInterfacePackets_0.roadPlane_x,
  SimulatorInterfacePackets_0.roadPlane_y,
  SimulatorInterfacePackets_0.roadPlane_z,
