  SimulatorInterfacePackets_0.wheelRevPerSecond_fr,
  SimulatorInterfacePackets_0.wheelRevPerSecond_rl,
  SimulatorInterfacePackets_0.wheelRevPerSecond_rr,
  SimulatorInterfacePackets_0.clutchPedal,
  SimulatorInterfacePackets_0.clutchEngagement,
  SimulatorInterfacePackets_0.rpmFromClutchToGearbox,
  SimulatorInterfacePackets_0.transmissionTopSpeed
FROM gt7_SimulatorInterfacePackets AS SimulatorInterfacePackets_0
WITH ASSOCIATIONS (
  MANY TO ONE JOIN GT7Service_Sessions AS session ON (session.ID = session_ID),
  MANY TO ONE JOIN GT7Service_Cars AS car ON (car.ID = car_ID)
)